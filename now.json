{
  "version": 2,
  "public": true,
  "builds": [
    {
      "src": "/references/dist/one/index.js",
      "use": "@now/node"
    },
    {
      "src": "/references/dist/two/index.js",
      "use": "@now/node"
    }
  ],
  "routes": [
    {
      "src": "/one(.*)",
      "dest": "/references/dist/one/index.js"
    },
    {
      "src": "/two(.*)",
      "dest": "/references/dist/two/index.js"
    }
  ],
  "github": {
    "autoAlias": true,
    "silent": true,
    "autoJobCancelation": true
  }
}
