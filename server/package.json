{
  "name": "@rewarp/server",
  "description": "Time tracking app",
  "repository": "https://github.com/vadistic/rewarp",
  "author": "Jakub Wadas <vadistic@gmail.com>",
  "version": "0.1.0",
  "license": "No License",
  "private": true,
  "engines": {
    "node": ">= 10.x"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "NODE_ENV=production nest build",
    "start:dev": "NODE_ENV=development nest start --watch",
    "start:debug": "NODE_ENV=development nest start --debug --watch",
    "start:prod": "node dist/apps/main/main",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "lint": "eslint libs/**/*.ts apps/**/*.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/main/test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^6.8.2",
    "@nestjs/core": "^6.8.2",
    "@nestjs/graphql": "^6.5.3",
    "@nestjs/platform-express": "^6.8.3",
    "@nestjs/typeorm": "^6.2.0",
    "@types/node": "12.7.12",
    "cache-manager": "^2.10.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.10.1",
    "dataloader": "^1.4.0",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "graphql": "^14.5.8",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.0",
    "rxjs": "^6.5.3",
    "typeorm": "^0.2.19"
  },
  "devDependencies": {
    "@nestjs/cli": "^6.9.0",
    "@nestjs/schematics": "^6.7.0",
    "@nestjs/testing": "^6.7.1",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.1",
    "typescript-transform-paths": "^1.1.11"
  }
}
