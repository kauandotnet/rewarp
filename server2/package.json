{
  "name": "@rewarp/server2",
  "description": "Time tracking app",
  "repository": "https://github.com/vadistic/rewarp",
  "author": "Jakub Wadas <vadistic@gmail.com>",
  "version": "0.1.0",
  "license": "No License",
  "private": true,
  "scripts": {
    "clean:logs": "rm *.log",
    "clean:build": "rm -rf dist",
    "build": "tsc -p tsconfig.build.json",
    "start": "ts-node src/server.ts",
    "start:dev": "tsc-watch -p tsconfig.build.json --onSuccess \"node dist/server.js\"",
    "start:debug": "tsc-watch -p tsconfig.build.json --onSuccess \"node --inspect-brk dist/server.js\"",
    "start:prod": "node dist/server.js",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint src/**/*.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r ts-node/register node_modules/.bin/jest --runInBand"
  },
  "engines": {
    "node": ">= 10.x"
  },
  "dependencies": {
    "@graphql-modules/core": "^0.7.13",
    "apollo-server-lambda": "^2.9.5",
    "apollo-server-micro": "^2.9.5",
    "dotenv": "^8.1.0",
    "typeorm": "^0.2.19"
  },
  "devDependencies": {
    "@types/dotenv": "^6.1.1",
    "@types/faker": "^4.1.5",
    "@types/jest": "24.0.18",
    "@types/node": "12.7.8",
    "@types/pg": "^7.11.2",
    "@typescript-eslint/eslint-plugin": "^2.3.1",
    "@typescript-eslint/parser": "^2.3.1",
    "apollo-server": "^2.9.5",
    "env-cmd": "^10.0.1",
    "eslint": "^6.5.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-import": "^2.18.2",
    "jest": "^24.9.0",
    "prettier": "1.18.2",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.4.1",
    "tsc-watch": "^4.0.0",
    "typescript": "^3.7.0-beta"
  }
}
