{
  "name": "@rewarp/server2",
  "description": "Time tracking app",
  "repository": "https://github.com/vadistic/rewarp",
  "author": "Jakub Wadas <vadistic@gmail.com>",
  "version": "0.1.0",
  "license": "No License",
  "private": true,
  "scripts": {
    "env": "env-cmd -f ../.env.development",
    "clean": "rm *.log && rm -rf dist",
    "start:dev": "bash scripts/start-dev.sh",
    "start:debug": "bash scripts/start-debug.sh",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint src/**/*.ts",
    "test": "yarn env jest",
    "test:watch": "yarn env jest --watch",
    "test:cov": "yarn env jest --coverage",
    "test:debug": "yarn env node --inspect-brk -r ts-node/register node_modules/.bin/jest --runInBand",
    "codegen": "ts-node -P tsconfig.json scripts/codegen"
  },
  "engines": {
    "node": ">= 10.x"
  },
  "dependencies": {
    "@graphql-modules/core": "^0.7.13",
    "@graphql-modules/di": "^0.7.13",
    "@rewarp/database": "0.1.0",
    "apollo-server": "^2.9.5",
    "apollo-server-lambda": "^2.9.5",
    "apollo-server-micro": "^2.9.5",
    "callsites": "^3.1.0",
    "dataloader": "^1.4.0",
    "dotenv": "^8.1.0",
    "globby": "^10.0.1",
    "graphql": "^14.5.8",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.19"
  },
  "devDependencies": {
    "@graphql-codegen/add": "^1.8.1",
    "@graphql-codegen/cli": "^1.8.1",
    "@graphql-codegen/schema-ast": "^1.8.1",
    "@graphql-codegen/typescript": "1.8.1",
    "@graphql-codegen/typescript-resolvers": "1.8.1",
    "@now/node": "^1.0.1",
    "@types/dotenv": "^6.1.1",
    "@types/faker": "^4.1.5",
    "@types/pg": "^7.11.2"
  }
}
